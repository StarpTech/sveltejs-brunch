  ],
  "author": "Dustin Deus <deusdustin@gmail.com> (http://www.starptech.de)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StarpTech/sveltejs-brunch/issues"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "homepage": "https://github.com/StarpTech/sveltejs-brunch#readme",
  "dependencies": {
    "combine-source-map": "^0.8.0",
    "svelte": "^3.4.4"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "np": "^5.0.2",
    "prettier": "^1.17.1"
